<Window x:Class="PL.ListOfDronesViewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="ListOfDronesViewWindow" Height="640" Width="800">
    <Grid Name="MainGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="50*"/>
            <RowDefinition Height="400*"/>
        </Grid.RowDefinitions>
        <ComboBox Name="StatusSelector" HorizontalAlignment="Stretch" Margin="182,58,0,0" Grid.Row="1" Grid.Column="1" VerticalAlignment="Stretch" Width="120" Height="34"/>
        <ComboBox Name="WeightSelecter"  HorizontalAlignment="Stretch" Margin="459,58,0,0" Grid.Row="1" Grid.Column="1" VerticalAlignment="Stretch" Width="120" Height="34"/>
        <TextBox Name="TBD" HorizontalAlignment="Left" Margin="120,104,0,0" Grid.Row="1" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="170" Height="27">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.941"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <ListView x:Name="ListOfDronesView" Grid.Row="1" Margin="94,243,445,158">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
